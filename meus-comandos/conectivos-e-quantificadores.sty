% Relações Binárias Elemento - Conjunto e Conjunto - Conjunto

% Elemento - Conjunto
\newcommand{\Pertinente}[2]{{#1}\in{#2}}     % Como usar: $ \Pertinente{Elm}{Set} $
\newcommand{\NPertinente}[2]{{#1}\notin{#2}} % Como usar: $ \NPertinente{Elm}{Set} $
\newcommand{\PertinenteR}[2]{{#1}\ni{#2}}    % Como usar: $ \PertinenteR{Set}{Elm} $

% Conjunto - Conjunto
\newcommand{\Contido}[2]{{#1}\subseteq{#2}}    % Como usar: $ \Contido{Subset}{Set} $
\newcommand{\PContido}[2]{{#1}\subsetneq{#2}}  % Como usar: $ \PContido{Set}{Subset} $
\newcommand{\NContido}[2]{{#1}\not\subset{#2}} % Como usar: $ \NContido{Set}{Set} $

% Quantificadores
%\newcommand{\ParaTodo}[2]{\forall\,{#1}\in{#2}} % Como usar: $ \ParaTodo{Elm}{Set} $